{
  "mutate": [
    "tracker/static/js/**/*.js",
    "!tracker/static/js/**/__tests__/**",
    "!tracker/static/js/**/vendor/**"
  ],
  "packageManager": "npm",
  "reporters": ["clear-text", "progress"],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "config": {
      "testEnvironment": "@stryker-mutator/jest-runner/jest-env/jsdom-sixteen"
    }
  },
  "coverageAnalysis": "perTest",
  "timeoutMS": 10000,
  "concurrency": 4,
  "thresholds": {
    "high": 85,
    "low": 65,
    "break": 50
  },
  "disableTypeChecks": "false"
}