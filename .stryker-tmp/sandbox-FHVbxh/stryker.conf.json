{
  "mutate": [
    "tracker/static/js/**/*.js",
    "!tracker/static/js/**/__tests__/**",
    "!tracker/static/js/**/vendor/**"
  ],
  "packageManager": "npm",
  "reporters": ["clear-text", "progress", "html"],
  "testRunner": "jest",
  "jest": {
    "projectType": "create-react-app"
  },
  "coverageAnalysis": "perTest",
  "timeoutMS": 10000,
  "concurrency": 4,
  "thresholds": {
    "high": 85,
    "low": 65,
    "break": 50
  }
}